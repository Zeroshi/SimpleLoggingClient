<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Thomas Nelson</Authors>
    <Description>Simple logging client. Used to log items directly to the platform, via Console.WriteLine(). Also sends messages to RabbitMQ to be picked up by a service that will break down the messages for analysis as needed.

Environment Variables:
"Queue Hostname"
"Queue Username"
"Queue Password"
"Queue Virtual Host"
"Queue Port"
"Queue Echange Name"
"Queue Routing Key"
"Queue Name"
"Logging Level" (int 0-3) Debug, Info, Error, Off
"Application Name"</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/Zeroshi/SimpleLoggingClient</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageTags>Simple, Logging, Client, RabbitMQ, Enterprise, Cloud</PackageTags>
    <PackageReleaseNotes>Bug Fix</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.9</Version>
    <NeutralLanguage></NeutralLanguage>
    <AssemblyVersion>1.0.9.0</AssemblyVersion>
    <FileVersion>1.0.9.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
    <PackageReference Include="RabbitMQ.Client" Version="5.1.0" />
    <PackageReference Include="SimpleLoggingInterfaces" Version="1.1.2" />
  </ItemGroup>
</Project>